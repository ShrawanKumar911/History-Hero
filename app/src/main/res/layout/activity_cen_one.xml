    <LinearLayout
        android:layout_width="match_parent"
        android:background="@color/yellow"
        android:layout_height="50dp">

    <TextView
        android:id="@+id/centuryNumber"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="1100 CENTURY"
        android:textSize="30dp"
        android:textStyle="bold"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="10dp"></TextView>
    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:id="@+id/enter1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_login"
        android:backgroundTint="@color/Parchment"
        app:cardCornerRadius="20dp"
        android:layout_margin="9dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/heroimage1"
                android:layout_width="102dp"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:src="@drawable/prithavirajchouhaniii"></ImageView>

            <LinearLayout
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/heroname1"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:text="Prithviraja III"
                    android:textSize="20sp"></TextView>
                <TextView
                    android:id="@+id/herodate1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Birth Date : 1166"
                    android:textSize="15dp"></TextView>

                <TextView
                    android:id="@+id/heroplace1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Birth Place : Rajasthan"
                    android:textSize="15dp"></TextView>
            </LinearLayout>

            <ImageView
                android:layout_margin="10dp"
                android:layout_width="60dp"
                android:layout_height="69dp"
                android:background="@drawable/baseline_arrow_forward_ios_24"></ImageView>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

